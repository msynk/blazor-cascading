@rendermode InteractiveServer

<h1>Cascading</h1>

<h3>Name: [@Name]</h3>
<h3>Age: [@Age]</h3>
<h3>Gender: [@Gender]</h3>

@code {
    [CascadingParameter(Name = "Gender")]
    public string Gender { get; set; }

    [CascadingParameter(Name = "Age")]
    public int Age { get; set; }


    [CascadingParameter(Name = "Name")]
    public string Name { get; set; }
}
